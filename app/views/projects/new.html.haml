%h1 Submit Project
%p
  Name: 
  = @project.title
%p
  URL: 
  = link_to @project.url, @project.url
= form_for @project, url: { action: "create" } do |f|
  = hidden_field_tag 'title', @project.title
  = hidden_field_tag 'url', @project.url
  = f.text_area :description, size: "60x12"
  = f.submit "Submit Project"
